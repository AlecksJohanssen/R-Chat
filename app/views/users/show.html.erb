<h2>Friends</h2>
<ul>
  <% @friendships.each do |friendship|  %>
    <li>
      <%= friendship.friend.name %>
      <%= link_to "remove", friendship, :method => :delete %>
    </li>
  <% end %>
</ul>

<p><%= link_to "Find Friends", users_path %></p>

<h2>Friended by Users</h2>
<ul>

  <% @users.each do |user|  %>
   <% if user.id != current_user.id %>
    <li><%= user.name %></li>
  <% end %>
  <% end %>
</ul>