
<div class="container">
	<h1>Log In </h1>
	<%= form_tag sessions_path do %>
	
<div class="form-control">
	<%= text_field_tag :email, "",placeholder: 'Input your Email Here' %>
</div>

<div class="form-control" style="margin-top:30px">
	<%= password_field_tag :password,"", placeholder: 'Input your Password Here' %>
</div>
<div class="move" style="margin-top:10px">
	<%= submit_tag "Submit", class: "btn btn-info"%>
</div>
</div>

<% end %>